{
  "name": "deprecation-notice",
  "main": "./lib/deprecation-notice",
  "version": "0.1.0",
  "description": "A way to add hints that modules being used are currently deprecated",
  "keywords": [],
  "repository": "https://github.com/jcblw/atom-deprecation-notice",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "atom-linter": "^4.3.0"
  },
  "package-deps": [
    "linter"
  ],
  "providedServices": {
    "linter": {
      "versions": {
        "1.0.0": "provideLinter"
      }
    }
  },
  "deprecated": {
    "deprecation-checker": "isDeprecated"
  }
}
